{
    "name": "localdisk/cast-formrequest",
    "description": "Cast FormRequest Attributes.",
    "keywords": ["laravel", "formrequest"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "localdisk",
            "email": "smoochynet@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "3.6.*",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Localdisk\\Request\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Localdisk\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
